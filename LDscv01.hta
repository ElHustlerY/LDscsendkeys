        <title>Send Keys</title>
		
	


		
		
        <script type="text/vbscript">
          
		  
		  	    
            Sub size50_OnClick()
				Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "{ENTER}-s 50{ENTER}"
			             End With
            End Sub

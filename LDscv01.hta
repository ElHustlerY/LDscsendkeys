			Sub FPS_OnClick()
                Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "^%f"
					     End With
            End Sub	
			
		
			Sub sleep (Timesec)
			Set objwsh = CreateObject("WScript.Shell") 
			objwsh.Run "Timeout /T " & Timesec & " /nobreak" ,0 ,true
			Set objwsh = Nothing
			End Sub
		
			
		
		
			Sub Bank_OnClick()
				
				Dim Wshell: Set WShell = CreateObject("WScript.Shell")
				With WShell 
				.AppActivate "StarCraft II"
			    .SendKeys "+f"
				End With
				
				
				End Sub	
	
			
			

    
    

        </script>
	<script type="text/javascript">
	var Xcountdown;
	var XTime;
	function Xinit() {
    XTime = 11;
    XReset();
    XTrigger();
}

function XTrigger() {
    if(XTime > 0) {
        XTime--;
        document.getElementById('XTimer').innerHTML = XTime;
        if(XTime > 0) {
            Xcountdown = setTimeout('XTrigger()', 1000);
        }}}
function XReset() {
    clearTimeout(Xcountdown);
}

var XDcountdown;
	var XDTime;
	function XDinit() {
    XDTime = 311;
    XDReset();
    XDTrigger();
}

function XDTrigger() {
    if(XDTime > 0) {
        XDTime--;
        document.getElementById('XDTimer').innerHTML = XDTime;
        if(XDTime > 0) {
            XDcountdown = setTimeout('XDTrigger()', 1000);
        }}}
function XDReset() {
    clearTimeout(XDcountdown);
}



	

</script>


	
   </head>
    <body>
	
		<button id="size50">50</button>
		<button id="size60">60</button>
		<button id="size70">70</button>
		<button id="size80">80</button>
		<button id="size90">90</button>
		<button id="size100">100</button>
			<br>
		<br>	
		<button id="data">@data+"CTRL+K"</button>
		<br>
		<br>
		<button id="nf">@nf</button>
		<button id="yf">@yf</button>
		<br>
		<br>
		<button id="rl">@rl</button>
		
		<button id="pt">@pt</button>
		
		<button id="upt">@upt</button>
		
		<br><br>
		
		
		<button onclick="Xinit()" id="x">@X</button>
		<p id="XTimer"></p>
		<button onclick="XDinit()" id="XD">@XD</button>
		<p id="XDTimer"></p>
		<button id="sxd">@SXD</button>
		
		<button id="FPS">Show/Hide FPS</button>

		<button id="Bank">AFK Fill The Bank</button>
		<input type="text" value="" id="AFKTimer">
		
    </body>

<!doctype html>
<html>

        <head>
        <meta http-equiv="content-type" content="text/html; charset=UTF-8">
        <title>Send Keys</title>
		
		
		
		
        <script type="text/vbscript">
          
		  
		  	    
            Sub size50_OnClick()
				Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "{ENTER}-s 50{ENTER}"
			             End With
            End Sub
			Sub size60_OnClick()
                Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "{ENTER}-s 60{ENTER}"
			             End With
            End Sub
			Sub size70_OnClick()
                Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "{ENTER}-s 70{ENTER}"
			             End With
            End Sub
			Sub size80_OnClick()
                Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "{ENTER}-s 80{ENTER}"
			             End With
            End Sub
			Sub size90_OnClick()
                Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "{ENTER}-s 90{ENTER}"
			             End With
            End Sub
			Sub size100_OnClick()
                Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "{ENTER}-s 100{ENTER}"
			             End With
            End Sub
			
			Sub data_OnClick()
                Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "{ENTER}@data{ENTER}"
						.SendKeys "^K"
			             End With
            End Sub
			
			Sub RL_OnClick()
                Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "{ENTER}@rl{ENTER}"
					     End With
            End Sub		
			
			Sub X_OnClick()
                Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "{ENTER}@x{ENTER}"
					     End With
            End Sub	
			
			Sub XD_OnClick()
                Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "{ENTER}@xd{ENTER}"
					     End With
            End Sub	
			
			Sub SXD_OnClick()
                Dim WShell : Set WShell = CreateObject("WScript.Shell")
                With WShell
	                    .AppActivate "StarCraft II"
                        .SendKeys "{ENTER}@sxd{ENTER}"
					     End With
            End Sub	
			
        </script>
	

	
   </head>
    <body>
	
	
        <button id="size50">50</button>
		<button id="size60">60</button>
		<button id="size70">70</button>
		<button id="size80">80</button>
		<button id="size90">90</button>
		<button id="size100">100</button>
				<br>
				<br>
		<button id="data">@data+insta in</button>
		<br>
		<br>
		<button id="rl">@RL</button>
		<br><br>
		<button id="xd">@X</button>
		<button id="X">@XD</button>
		<button id="sxd">@SXD</button>



    </body>
</html>